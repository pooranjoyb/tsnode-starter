{
    "name": "tsnode-starter",
    "version": "1.0.0",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
      "build": "tsc",
      "start": "node --experimental-specifier-resolution=node dist/index.js",
      "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts"
    },
    "dependencies": {
      "express": "^4.17.1"
    },
    "devDependencies": {
      "typescript": "^4.5.5",
      "ts-node": "^10.4.0",
      "@types/node": "^17.0.5",
      "@types/express": "^4.17.13",
      "nodemon": "^2.0.15"
    }
  }
  